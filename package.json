{
  "name": "harpernet.org",
  "version": "1.3.2",
  "description": "This page is intended to give you insights into Jungian psychology and the MBTI framework. Maybe something you haven't yet considered.",
  "type": "commonjs",
  "scripts": {
    "dev": "vite --port 3890",
    "build": "vite build",
    "preview": "vite preview --port 3890",
    "serve": "vite preview --port 3890 --host",
    "start": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop mbti-app",
    "restart": "pm2 restart mbti-app",
    "delete": "pm2 delete mbti-app",
    "status": "pm2 status mbti-app",
    "logs": "pm2 logs mbti-app",
    "watch": "nodemon --watch src --exec \"npm run dev\" --ext js,jsx,css,html",
    "debug": "vite --port 3890 --debug --host",
    "bump": "npm version patch",
    "prepare": "husky",
    "format": "prettier --write .",
    "check-format": "prettier --check ."
  },
  "keywords": [
    "mbti",
    "personality",
    "psychology"
  ],
  "dependencies": {
    "solid-js": "^1.9.9"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.8"
  },
  "author": "soulwax@github",
  "license": "GPL-3.0",
  "homepage": "harpernet.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/soulwax/harpernet.org.git"
  },
  "technology": "Solid.JS",
  "technologyHomepage": "https://solidjs.com"
}
